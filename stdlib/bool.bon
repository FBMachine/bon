impl Print<bool>
  def print(x:bool) -> ()
    match x
      true => print("true")
      false => print("false")
    end
  end

  def write(x:bool) -> ()
    match x
      true => write("true")
      false => write("false")
    end
  end

  def to_string(x:bool) -> string
    match x
      true => "true"
      false => "false"
    end
  end
end
